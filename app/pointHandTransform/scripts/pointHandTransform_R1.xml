<application>
   <name>pointHandTransform_R1</name>

   <dependencies>
   </dependencies>

   <application>
      <name>Navigation_ROS2_R1SN003</name>
      <prefix></prefix>
   </application>

   <module>
      <name>pointHandTransform</name>
      <parameters>--context pointHandTransform --from pointHandTransform_R1.ini</parameters>
      <environment></environment>
      <node>console</node>
   </module>

   <module>
      <name>yarpview</name>
      <parameters>--name /view/cameraView:i --out /view/cameraClick:o </parameters>
      <node>console</node>
   </module>
   
   <module>
      <name>cer_reaching-solver</name>
      <parameters>--robot cer --arm-type right --verbosity 1</parameters>
      <node>console</node>
   </module>

   <module>
      <name>cer_reaching-controller</name>
      <parameters>--robot cer --arm-type right --verbosity 1 --T 1.0</parameters>
      <node>console</node>
   </module>

   <connection>
      <from>/cer/realsense_repeater/rgbImage:o</from>
      <to>/view/cameraView:i</to>
      <protocol>tcp</protocol>
   </connection>

   <connection>
      <from>/view/cameraClick:o</from>
      <to>/pointHandTransform/clicked_point:i</to>
      <protocol>tcp</protocol>
   </connection>

   <connection>
      <from>/cer_reaching-controller/right/solver:rpc</from>
      <to>/cer_reaching-solver/right/rpc</to>
      <protocol>tcp</protocol>
   </connection>

   <connection>
      <from>/pointHandTransform/target:o</from>
      <to>/cer_reaching-controller/right/target:i</to>
      <protocol>tcp</protocol>
   </connection>

</application>
